@model List<ExportDemo.Models.Person>

@{
    Layout = null;
}

<h1>Data Persons</h1>
<table border="1" cellpadding="5" cellspacing="0" style="border-collapse: collapse; width: 100%;">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nama</th>
            <th>Umur</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.Id</td>
                <td>@person.Nama</td>
                <td>@person.Umur</td>
            </tr>
        }
    </tbody>
</table>
